{
  "fullUrl": "urn:uuid:{{ resource.Patient.id }}",
  "resource": {
    "resourceType": "Patient",
    "id": "{{ resource.Patient.id }}",
    "meta": {
      "profile": [
        "http://hl7.org/fhir/us/core/StructureDefinition/us-core-patient"
      ]
    },
    "identifier": [
      {% include 'Templates/DataType/Identifier.liquid' with id=resource.Patient.id %}
    ],
    "name": [
      {% include 'Templates/DataType/HumanName.liquid' with name=resource.Patient.name %}
    ],
    "birthDate": "{{ resource.Patient.birthTime.value | format_date_time }}",
    "gender": "{{ resource.Patient.administrativeGenderCode.code.code | default: 'unknown' }}",
    "deceasedBoolean": "{{ resource.Patient.deceasedBoolean }}",
    "deceasedDateTime": "{{ resource.Patient.deceasedDateTime | format_date_time }}",
    "maritalStatus": {
      {% include 'Templates/DataType/CodeableConcept' CodeableConcept: resource.Patient.maritalStatusCode %}
    },
    "multipleBirthBoolean": "{{ resource.Patient.multipleBirthBoolean }}",
    "multipleBirthInteger": "{{ resource.Patient.multipleBirthInteger }}",
    "extension": [
      {% include 'Templates/Ccda/Extensions/omb-race-category.liquid' with raceCode=resource.Patient.raceCode %}
      {% include 'Templates/Ccda/Extensions/omb-ethnicity-category.liquid' with ethnicityCode=resource.Patient.ethnicityCode %}
    ],
    "address": [
      {% include 'Templates/DataType/Address.liquid' with addr=resource.Patient.addr %}
    ],
    "telecom": [
      {% include 'Templates/DataType/Telecom.liquid' with telecom=resource.Patient.telecom %}
    ],
    "communication": [
      {% include 'Templates/Ccda/Communication/Communication.liquid' with languageCommunication=resource.Patient.languageCommunication %}
    ],
    "photo": [
      {% include 'Templates/DataType/Attachment.liquid' Attachment: resource.Patient.photo %}
    ],
    "generalPractitioner": [
      {% include 'Templates/DataType/Reference.liquid' Reference: resource.Patient.generalPractitioner %}
    ],
    "managingOrganization": {
      {% include 'Templates/DataType/Reference.liquid' Reference: resource.Patient.managingOrganization %}
    }
  },
  "request": {
    "method": "PUT",
    "url": "Patient/{{ resource.Patient.id }}"
  }
}
